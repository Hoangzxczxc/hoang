<a class="btn btn-info" href="/toy/add">Add new toy</a>
<br><br>
<table class="table table-bordered">
   <tr>
      <th>Toy model</th>
      <th>Toy size</th>
      <th>Toy price</th>
      <th>Name of manufacturer</th>
      <th>Toy image</th>
   </tr>
   {{#each toys }}
   <tr>
      <td>{{ model }}</td>
      <td>{{ size }}</td>
      <td>{{ price }}</td>
      <td>{{ manufacturer.name }}</td>
      <td>
         <img src="{{ image }}" alt="" width="100" height="120">
      </td>
   </tr>
   {{/each}}
</table>